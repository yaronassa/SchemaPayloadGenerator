{
  "description": "Generate massive amounts of schema payloads via value combinations",
  "name": "schema-payload-generator",
  "author": {
    "name": "Yaron Assa",
    "email": "yaron.assa@gmail.com"
  },
  "version": "0.0.1",
  "scripts": {
    "test": "mocha -r ts-node/register -r ./test/setup/setup.ts test/**/*.ts",
    "coverage": "nyc mocha  -r ts-node/register -r source-map-support/register --full-trace --bail -r ./test/setup/setup.ts test/**/*.ts",
    "lint": "./node_modules/.bin/tslint -p ./"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "typescript": "^3.1.3",
    "json-schema-ref-parser": "^6.0.1",
    "assign-deep": "^1.0.0"
  },
  "devDependencies": {
    "ts-node": "^7.0.1",
    "@types/node": "^10.11.7",
    "@types/json-schema": "^7.0.1",
    "@types/mocha": "^5.2.5",
    "@types/chai": "^4.1.6",
    "@types/chai-as-promised": "^7.1.0",
    "tslint": "^5.10.0",
    "mocha": "^5.2.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "sinon": "^7.0.0",
    "sinon-chai": "^3.2.0",
    "@types/sinon": "^5.0.5",
    "@types/sinon-chai": "^3.2.0",
    "nyc": "^13.1.0",
    "source-map-support": "^0.5.9"
  }
}
